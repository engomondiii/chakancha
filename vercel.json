{
  "version": 2,
  "builds": [
    {
      "src": "chakancha/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "chakancha/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "chakancha.settings.production"
  }
}